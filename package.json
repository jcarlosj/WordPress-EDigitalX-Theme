{
  "name": "edigitalx",
  "version": "1.0.0",
  "description": "'Explicación Digital' Advanced WordPress Theme for Gutenberg",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "npm run clean:src; npm run clean:build",
    "clean:build": "rm -rf ./build/* ./assets/build/* ",
    "clean:src": "rm -rf ./assets/src/css/* ",
    "svg": "svgr -d ./assets/src/js/icons ./assets/src/icons",
    "lint": "eslint ./assets/src/js/*",
    "lint:fix": "eslint --fix ./assets/src/js/*",
    "lint-css": "stylelint ./assets/src/sass/**/**/*.scss",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm rin lint:fix && npm run lint-css:fix && npm run build"
  },
  "keywords": [
    "Advanced Theme",
    "Theme for Gutenberg",
    "Use Webpack",
    "Use Babel",
    "Use Sass"
  ],
  "author": "Juan Carlos Jiménez Gutiérrez <jcjimenez29@misena.edu.co>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@beyonk/google-fonts-webpack-plugin": "^1.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.1.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.24.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "raw-loader": "^4.0.2",
    "sass": "^1.35.2",
    "sass-loader": "^11.0.1",
    "sass-mq": "^5.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.6.0"
  }
}
