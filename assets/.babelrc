{
    "presets": [
        [
            "@babel/preset-env",    //  Ajuste preestablecido inteligente que permite hacer uso de sintaxis de la ultima version de JavaScript
            {
                "targets": {
                    "browsers": [
                        "last 2 Chrome versions",
                        "last 2 Firefox versions",
                        "last 2 Safari versions",
                        "last 2 iOS versions",
                        "last 1 Android version",
                        "last 1 ChromeAndroid version",
                        "ie 11"
                    ]
                }
            }
        ],
        "@babel/preset-react"   //  Ajuste para soportar React y JSX en tiempo de ejecución automático (Importante para la construcción de Bloques de Gutenberg en React)
    ]
}